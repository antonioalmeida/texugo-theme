{
  "name": "texugo-theme",
  "devDependencies": {
    "bulma": "^1.0.0",
    "sass": "^1.77.0"
  },
  "scripts": {
    "css-build": "sass --no-source-map --style=compressed scss/styles.scss static/css/styles.css",
    "css-watch": "sass --no-source-map --style=compressed --watch scss/styles.scss static/css/styles.css",
    "start": "npm run css-watch"
  }
}
